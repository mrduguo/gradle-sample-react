    "standard-format": "standard-format -w",
    "test": "jest"
  },
  "main": "src/index.js",
  "dependencies": {
    "autoprefixer": "~6.0.3",
    "babel-core": "~6.1.21",
    "babel-loader": "~6.1.0",
    "babel-preset-es2015": "~6.1.4",
    "babel-preset-react": "~6.1.4",
    "css-loader": "~0.23.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "node-sass": "~3.4.1",
    "postcss-loader": "~0.8.0",
    "react": "~0.14.1",
    "react-dom": "~0.14.6",
    "rest": "~1.3.1",
    "sass-loader": "~3.1.1",
    "sockjs-client": "~1.0.3",
    "stompjs": "~2.3.3",
    "style-loader": "~0.13.0",
    "webpack": "~1.12.6"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "~5.0.0",
    "babel-jest": "~9.0.1",
    "babel-register": "~6.1.0",
    "eslint-plugin-babel": "~3.1.0",
    "jest-cli": "^0.9.1",
    "jest-webpack-alias": "~2.2.0",
    "jestpack": "~0.2.0",
    "react-addons-test-utils": "~0.14.7",
    "standard": "~6.0.3",
    "standard-format": "~2.1.1",
