      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "it",
      "expect"
    ]
  },
  "jest-webpack-alias": {
    "configFile": "config/webpack.js"
